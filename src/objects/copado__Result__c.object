        <deprecated>false</deprecated>
        <description>Total duration in seconds of the job execution. This value can be used to track job performance.</description>
        <externalId>false</externalId>
        <formula>IF(
  AND(
    NOT(ISBLANK(copado__Start_Time__c)),
    NOT(ISBLANK(copado__End_Time__c))
  ),
  (copado__End_Time__c - copado__Start_Time__c)*24*60*60,
  NULL
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
